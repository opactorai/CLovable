{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@/backend-types": ["./backend-types/index.ts"],
      "@/backend-types/*": ["./backend-types/*"],
      "react-icons/fa": ["./stubs/react-icons-fa.tsx"],
      "react-icons/si": ["./stubs/react-icons-si.tsx"],
      "react-icons/vsc": ["./stubs/react-icons-vsc.tsx"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "data/projects",
    "external_Claudable"
  ]
}
