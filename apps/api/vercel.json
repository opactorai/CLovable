{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "API_PORT": "8080",
    "DATABASE_URL": "@database_url",
    "PROJECTS_ROOT": "/tmp/projects",
    "PREVIEW_PORT_START": "3100",
    "PREVIEW_PORT_END": "3999"
  }
}